name: mypins
version: 1.0.0
type: vite
scripts:
  dev: "vite"
  build: "tsc && vite build"
  preview: "vite preview"
dependencies:
  - react
  - react-dom
  - react-router-dom
  - firebase
  - @tiptap/react
  - @tiptap/starter-kit
  - lucide-react
  - @vitejs/plugin-react
  - typescript
  - vite
appFiles:
  entry: src/main.tsx
  html: index.html
  styles: src/index.css
  config:
    - vite.config.ts
    - tsconfig.json
    - package.json
    - postcss.config.js
    - tailwind.config.js
build:
  outDir: dist
  target: es2015
  port: 3000
